obj-$(CONFIG_HOBOT_MCU_IRQ)	+= mcu_irq/
obj-$(CONFIG_HOBOT_BIFSPI_DEV)		+= bifspi/
obj-$(CONFIG_HOBOT_BIFSD_DEV)		+= bifsd/
obj-$(CONFIG_HOBOT_BIF_BASE)		+= bif_base/
obj-$(CONFIG_HOBOT_BIF_ETHERNET)	+= bif_ethernet/
obj-$(CONFIG_HOBOT_BIF_SIO)		+= bif_sio/
obj-$(CONFIG_HOBOT_BIF_LITE_CORE)  	+= bif_lite/
obj-$(CONFIG_HOBOT_BIF_LITE_SPI)   	+= bif_lite/
obj-$(CONFIG_HOBOT_BIF_LITE_SD)    	+= bif_lite/
obj-$(CONFIG_HOBOT_HBIPC_ETH)    	+= bif_lite/
obj-$(CONFIG_HOBOT_SSCG)		+= sscg/
obj-$(CONFIG_HOBOT_DDR_MON)	+= ddr_monitor/
